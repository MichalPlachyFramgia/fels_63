<div class="row">
  <p id="notice"><%= notice %></p>
  <h1><%= t "admin.user.word_list" %></h1>
  <table class="table table-bordered table-striped">
    <thead>
      <tr>
        <th><%= t "title.content" %></th>
        <th><%= t "title.category" %></th>
      </tr>
    </thead>
    <tbody>
      <%= render @words %>
    </tbody>
  </table>
  <hr>
  <%= will_paginate @words %>
  <br>
  <%= link_to t("admin.user.new_word"), new_admin_word_url %>
  <%= render "import" %>
</div>
